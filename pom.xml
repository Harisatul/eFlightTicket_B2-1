    <distributionManagement>
        <repository>
            <id>local-release-repository</id>
            <name>Local Release Repository</name>
            <url>file:///C:\Users\Harisatul Aulia\.m2\repository</url>
        </repository>
    </distributionManagement>
    <scm>
        <connection>scm:git:git@github.com:mfyasykur/eFlightTicket_B2.git</connection>
